# Planyo API Configuration
# Copy this file to .env and fill in your actual values

# Planyo API Base URL
REACT_APP_PLANYO_API_URL=https://api.planyo.com

# Planyo API Key (get this from your Planyo account)
REACT_APP_PLANYO_API_KEY=your_api_key_here

# Optional: Site ID if you have multiple sites
REACT_APP_PLANYO_SITE_ID=your_site_id_here

# Optional: Environment
REACT_APP_ENV=development
